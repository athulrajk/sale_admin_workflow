<odoo>
    <record id="view_order_form_inherit_sale_admin" model="ir.ui.view">
        <field name="name">sale.order.form.inherit.sale.admin</field>
        <field name="model">sale.order</field>
        <field name="inherit_id" ref="sale.view_order_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='partner_id']" position="after">
                <field name="manager_reference" attrs="{'readonly': [('groups', 'not in', ['sale_admin_workflow.group_sale_admin'])]}"/>
                <field name="auto_workflow"/>
            </xpath>
        </field>
    </record>
</odoo>